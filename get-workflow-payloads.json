  "7_supabase_direct_query": {
    "method": "GET",
    "endpoint": "https://vxsjiwlvradiyluppage.supabase.co/rest/v1/workflows",
    "description": "Direct Supabase query (replace with actual workflow ID)",
    "url": "GET https://vxsjiwlvradiyluppage.supabase.co/rest/v1/workflows?id=eq.{workflow-id}&select=*",
    "headers": {
      "apikey": "YOUR_ANON_KEY",
      "Authorization": "Bearer YOUR_SERVICE_ROLE_KEY",
      "Content-Type": "application/json"
    },
    "payload": null,
    "example_curl": "curl -X GET 'https://vxsjiwlvradiyluppage.supabase.co/rest/v1/workflows?id=eq.123e4567-e89b-12d3-a456-426614174000&select=*' -H 'apikey: YOUR_ANON_KEY' -H 'Authorization: Bearer YOUR_SERVICE_ROLE_KEY'"
  },

